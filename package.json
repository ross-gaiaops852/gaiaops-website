{
  "name": "popular-phase",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "astro check",
    "sync": "astro sync",
    "lint": "eslint . --ext .astro,.js,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .astro,.js,.ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run lint:check && npm run format:check && npm run check",
    "deploy:dev": "npm run build && echo 'Ready for development deployment'",
    "deploy:prod": "npm run build && echo 'Ready for production deployment'"
  },
  "dependencies": {
    "astro": "^5.14.1",
    "heroicons": "^2.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.40.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
